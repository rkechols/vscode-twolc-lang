{
    "name": "twolc-lang",
    "displayName": "Twolc lang",
    "description": "Syntax highlighting for .twolc files",
    "version": "0.1.1",
    "publisher": "Echols021",
    "engines": {
        "vscode": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rkechols/vscode-twolc-lang.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "twolc",
                "aliases": [
                    "Twolc",
                    "twolc"
                ],
                "extensions": [
                    ".twolc",
                    ".twol"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "twolc",
                "scopeName": "source.twolc",
                "path": "./syntaxes/twolc.tmLanguage.json"
            }
        ]
    }
}
